@model List<AspStudio_Boilerplate.Areas.Users.Models.IndexViewModel>;

@{
    ViewData["Title"] = "Manage Users";
}

<div>

<table class="table">
<thead>
<tr>
    <th scope="col">@Html.DisplayNameFor(x => x[0].FirstName)</th>
    <th scope="col">@Html.DisplayNameFor(x => x[0].LastName)</th>
    <th scope="col">@Html.DisplayNameFor(x => x[0].Email)</th>
</tr>
</thead>

<tbody>
@foreach (var item in Model)
{

    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
    </tr>
}
</tbody>
    </table>
</div>